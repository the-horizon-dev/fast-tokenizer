{
  "name": "@the-horizon-dev/fast-tokenizer",
  "version": "1.3.1",
  "description": "High-performance library for tokenizing text.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:prod": "npm run build && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest --preset ts-jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-horizon-dev/fast-tokenizer.git"
  },
  "keywords": [
    "tokenizer",
    "tokenization",
    "text processing",
    "NLP",
    "natural language processing",
    "high-performance",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "Maximiliano Veiga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-horizon-dev/fast-tokenizer/issues"
  },
  "homepage": "https://github.com/the-horizon-dev/fast-tokenizer#readme",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@the-horizon-dev/fast-diacritics": "^1.1.0"
  }
}
